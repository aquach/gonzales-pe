    },
    "main": "./lib/gonzales",
    "repository": {
        "type": "git",
        "url": "http://github.com/tonyganch/gonzales-pe.git"
    },
    "scripts": {
        "build": "./node_modules/.bin/babel --loose all --compact true --comments false --blacklist spec.functionName src --out-dir lib",
