    "version": "3.0.0-30",
    "homepage": "http://github.com/tonyganch/gonzales-pe",
    "bugs": "http://github.com/tonyganch/gonzales-pe/issues",
    "license": "MIT",
    "author": {
        "name" : "Tony Ganch",
        "email" : "tonyganch+github@gmail.com",
